<FORMATS>

      <FORMAT NAME="asyncexecution">
	<REGEXP PATTERN="(^|[ \n]+)[iI][nN][tT][eE][rR][rR][uU][pP][tT][ \t]+[ \[\]\$\t\d\D\w\W]+($|[ \n]+)" ></REGEXP>
	<COLOR FOREGROUND="#ff50ff" ></COLOR>
      </FORMAT>
     
      <FORMAT NAME="tipo1">
	  <REGEXP PATTERN="(^|[ \n]+)E6POS($|[ \n]+)" ></REGEXP>
	  <REGEXP PATTERN="(^|[ \n]+)E6AXIS($|[ \n]+)" ></REGEXP>
	  <REGEXP PATTERN="(^|[ \n]+)INT($|[ \n]+)" ></REGEXP>
	  <COLOR FOREGROUND="#0000EF" ></COLOR>
      </FORMAT>
     
     <FORMAT NAME="sysvar">
	<REGEXP PATTERN="[\$]{1}([0-9_]*[a-zA-Z]+[0-9_]*)+([\[]{1}[ ]*[0-9]+[ ]*[\]]{1})*" ></REGEXP>
	<COLOR FOREGROUND="#ff8010" ></COLOR>
     </FORMAT>
     
     <FORMAT NAME="digit">
	  <REGEXP PATTERN="[ |^|\t]+[\d]+(\.([\d])+){0,1}[ |$|\t]{1}" ></REGEXP>
	  <COLOR FOREGROUND="#ff0000" ></COLOR>
      </FORMAT>

     <FORMAT NAME="statement">
		<REGEXP PATTERN="(^|[ \n]+)[pP][tT][pP]($|[ \n]+)" ></REGEXP>
		<REGEXP PATTERN="(^|[ \n]+)[lL][iI][nN]($|[ \n]+)" ></REGEXP>
		<REGEXP PATTERN="(^|[ \n]+)[lL][iI][nN]_[rR][eE][lL]($|[ \n]+)" ></REGEXP>
		<REGEXP PATTERN="(^|[ \n]+)[cC][iI][rR][cC]($|[ \n]+)" ></REGEXP>
		<REGEXP PATTERN="(^|[ \n]+)[cC]_[dD][iI][sS]($|[ \n]+)" ></REGEXP>
		<REGEXP PATTERN="(^|[ \n]+)[cC]_[pP][tT][pP]($|[ \n]+)" ></REGEXP>
		<REGEXP PATTERN="(^|[ \n]+)[bB][rR][aA][kK][eE]($|[ \n]+)" ></REGEXP>
		<REGEXP PATTERN="(^|[ \n]+)[fF][oO][rR]($|[ \n]+)" ></REGEXP>
		<REGEXP PATTERN="(^|[ \n]+)[hH][aA][lL][tT]($|[ \n]+)" ></REGEXP>
		<COLOR FOREGROUND="#ddbb00" ></COLOR>
	</FORMAT>
     <FORMAT NAME="comment">
		<REGEXP PATTERN=";([\s\S]*)*" ></REGEXP>
		<COLOR FOREGROUND="#70BB50"></COLOR>
	</FORMAT>
 </FORMATS>
